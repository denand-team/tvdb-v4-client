{
    "name": "denand/tvdb-v4-client",
    "description": "TVDB V4 API Client for PHP",
    "keywords": [
        "denand",
        "tvdb-v4-client"
    ],
    "homepage": "https://github.com/denand/tvdb-v4-client",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "DenAnd",
            "email": "denandteam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Denand\\TvdbV4Client\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Denand\\TvdbV4Client\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Denand\\TvdbV4Client\\TvdbV4ClientServiceProvider"
            ],
            "aliases": {
                "TvdbV4Client": "Denand\\TvdbV4Client\\TvdbV4ClientFacade"
            }
        }
    }
}
